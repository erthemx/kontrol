            if (txtKullanıcıAd.Text == "admin" && password.Text == "2131")
            {
                MessageBox.Show("giriş başarılı");
            }

            else
            {
                MessageBox.Show("HATALI");
            }
        }
        private void btnclose_Click(object sender, EventArgs e)
        {
            this.Close();
            Application.Exit();
        }
